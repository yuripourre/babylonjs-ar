{
  "name": "babylonjs-ar",
  "version": "2.0.0",
  "description": "High-performance WebGPU AR library for Babylon.js with marker tracking, plane detection, and environment estimation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./babylon": {
      "import": "./dist/adapters/babylon/index.js",
      "types": "./dist/adapters/babylon/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=browser --format=esm --sourcemap=external && bun run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "bun run dev-server.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "bench": "bun run benchmarks/index.ts",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.{ts,json}' 'tests/**/*.{ts,json}' '*.{json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,json}' 'tests/**/*.{ts,json}' '*.{json,md}'",
    "typecheck": "tsc --noEmit",
    "validate": "bun run format:check && bun run lint && bun run typecheck && bun test",
    "docs:api": "typedoc --out docs/api src/index.ts",
    "prepublishOnly": "bun run validate && bun run build"
  },
  "keywords": [
    "ar",
    "augmented-reality",
    "webgpu",
    "babylonjs",
    "aruco",
    "marker-tracking",
    "plane-detection",
    "computer-vision",
    "6dof"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@babylonjs/core": "^7.0.0",
    "three": "^0.150.0"
  },
  "peerDependenciesMeta": {
    "@babylonjs/core": {
      "optional": true
    },
    "three": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@webgpu/types": "^0.1.51",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typedoc": "^0.26.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
